{
  "schemaVersion": 1,
  "id": "twodimensional",
  "version": "${version}",
  "name": "Two Dimensional",
  "description": "Removes the third dimension from the game (Made for itsjim 2025 competition)",
  "authors": ["Mishkis"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/twodimensional/icon.png",
  "environment": "*",
  "entrypoints": {
    "client":[
      "github.mishkis.twodimensional.client.TwoDimensionalClient"
    ],
    "main": [
      "github.mishkis.twodimensional.TwoDimensional"
    ]
  },
  "mixins": [
    "twodimensional.mixins.json",
    {
      "config": "twodimensional.client.vanilla.mixins.json",
      "environment": "client"
    },
    {
      "config": "twodimensional.client.sodiumcompat.mixins.json",
      "environment": "client",
      "mods": ["sodium"]
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
